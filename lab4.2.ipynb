# Створюємо групи S4, A4, S5, A5
S4 = SymmetricGroup(4)
A4 = AlternatingGroup(4)
S5 = SymmetricGroup(5)
A5 = AlternatingGroup(5)


#  S4
print("Підгрупи S4:")
for H in S4.subgroups():
    print(H)

# A4
print("\nПідгрупи A4:")
for H in A4.subgroups():
    print(H)

#  S5
print("\nПідгрупи S5:")
for H in S5.subgroups():
    print(H)

#  A5
print("\nПідгрупи A5:")
for H in A5.subgroups():
    print(H)
