G = SymmetricGroup(3)

order_G = G.order()
print(f"Порядок групи S3: {order_G}")

divisors = divisors(order_G)
print(f"Дільники порядку: {divisors}")

# Знаходимо підгрупи G
subgroups = G.subgroups()
print("Підгрупи S3:")
for H in subgroups:
    print(f"Підгрупа: {H}, Порядок: {H.order()}")

exists_subgroup_order_3 = any(H.order() == 3 for H in subgroups)
print(f"Чи існує підгрупа порядку 3: {exists_subgroup_order_3}")
